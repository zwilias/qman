{
  "name": "zwilias/qman",
  "type": "library",
  "description": "An evented beanstalkd queue manager",
  "keywords": ["beanstalk", "beanstalkd", "queue", "closure", "worker", "job", "libev", "eventloop"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ilias Van Peer",
      "email": "ilias@idlemonkeys.net"
    }
  ],
  "require": {
    "zwilias/beanie": "~0.1",
    "jeremeamia/superclosure": "^2.1",
    "psr/log": "^1.0",
    "php": ">=5.5.0",
    "ext-ev": "^0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "phpunit/php-invoker": "^1.1",
    "scrutinizer/ocular": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "QMan\\": "src/"
    }
  },
  "suggest": {
    "monolog/monolog": "Allows for powerful, granular logging. QMan has full support for psr/log, which this provides."
  }
}
